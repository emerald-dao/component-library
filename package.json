{
	"name": "@emerald-dao/component-library",
	"homepage": "http://emerald-dao.github.io/component-library",
	"version": "0.0.0-development",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"package": "svelte-package",
		"preview": "svelte-kit preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"semantic-release": "semantic-release",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"predeploy": "npm run build-storybook",
		"deploy-storybook": "gh-pages -d storybook-static"
	},
	"exports": {
		".": "./index.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@babel/core": "^7.19.0",
		"@iconify/svelte": "^2.2.1",
		"@semantic-release/changelog": "^6.0.1",
		"@semantic-release/git": "^10.0.1",
		"@storybook/addon-actions": "^6.5.13",
		"@storybook/addon-essentials": "^6.5.13",
		"@storybook/addon-interactions": "^6.5.13",
		"@storybook/addon-links": "^6.5.13",
		"@storybook/addon-svelte-csf": "^2.0.10",
		"@storybook/builder-vite": "^0.2.5",
		"@storybook/svelte": "^6.5.13",
		"@storybook/testing-library": "^0.0.13",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "^1.0.0",
		"@sveltejs/package": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.7",
		"babel-loader": "^8.2.5",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-storybook": "^0.6.7",
		"eslint-plugin-svelte3": "^4.0.0",
		"gh-pages": "^4.0.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.54.9",
		"semantic-release": "^19.0.5",
		"storybook-addon-theme-switcher": "^0.0.5",
		"svelte": "^3.50.1",
		"svelte-check": "^2.7.1",
		"svelte-loader": "^3.1.3",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@emerald-dao/design-system": "^2.0.0",
		"@mateoroldos/svelte.bones": "^1.2.0",
		"cssnano": "^5.1.14"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/emerald-dao/component-library.git"
	}
}
